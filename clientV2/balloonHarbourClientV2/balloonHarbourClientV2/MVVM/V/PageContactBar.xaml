<Page x:Class="balloonHarbourClientV2.MVVM.V.PageContactBar"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:balloonHarbourClientV2.MVVM.V"
      mc:Ignorable="d" 
      d:DesignHeight="650" d:DesignWidth="150"
      Title="FrameContactBar">
    
    <ListView x:Name="ListViewContacts"
                  ItemsSource="{Binding Contacts}"
                  Grid.Row="1"
                  BorderThickness="0"
                  ItemContainerStyle="{StaticResource Contactcard}"/>
</Page>
